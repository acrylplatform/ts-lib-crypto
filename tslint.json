{
  "defaultSeverity": "error",
  "extends": [],
  "jsRules": {},
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "arrow-return-shorthand": [
      true,
      "multiline"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "indent": [true, "spaces", 2],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ]
  },
  "rulesDirectory": []
}
